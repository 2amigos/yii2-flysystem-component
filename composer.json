{
    "name": "2amigos/yii2-flysystem-component",
    "description": "The League Flysystem integration for Yii Framework",
    "type": "yii2-extension",
    "keywords": [
        "2amigos",
        "yii",
        "yii2",
        "yii 2",
        "flysystem",
        "filesystem",
        "files",
        "aws",
        "s3",
        "azure",
        "dropbox",
        "ftp",
        "gridfs",
        "rackspace",
        "sftp",
        "webdav"
        "extension",
        "yii2-extension",
        "yii2-flysystem-component"
    ],
    "homepage": "https://github.com/2amigos/yii2-flysystem-component",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "2amigOS! Consulting Group",
            "email": "hola@2amigos.us",
            "homepage": "http://2amigos.us",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/2amigos/yii2-flysystem-component/issues",
        "source": "https://github.com/2amigos/yii2-flysystem-component"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/yii2": "*",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "squizlabs/php_codesniffer": "*",
        "phpmd/phpmd": "@stable",
        "league/flysystem-aws-s3-v3": "^1.0",
        "spatie/flysystem-dropbox": "^1.0",
        "league/flysystem-rackspace": "^1.0",
        "league/flysystem-gridfs": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "league/flysystem-webdav": "^1.0",
        "league/flysystem-ziparchive": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\flysystem\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
